.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  background: #fff;
  z-index: 999;
}
.custom text{
  width: 100%;
  height: 45px;
  line-height: 45px;
  
  text-align: center;
  position: absolute;
  color: #000;
  font-size: 48rpx;
  font-weight: 500;
 
}
.empty_custom{
  height: 45px;
  width: 100%;
}